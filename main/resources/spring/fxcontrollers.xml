           http://www.springframework.org/schema/context
           http://www.springframework.org/schema/context/spring-context-3.1.xsd">
    <context:annotation-config/>
    <bean name="fleetManagementScreen" class="com.newdawn.gui.fleet.FleetManagementScreen"/>
    <bean name="mainScreen" class="com.newdawn.gui.MainScreen"/>
    <bean name="systemMapScreen" class="com.newdawn.gui.map.system.SystemMapScreen"/>
    <bean name="colonyEconomicScreen" class="com.newdawn.gui.economic.ColonyEconomicScreen"/>
    <bean name="economicScreen" class="com.newdawn.gui.economic.EconomicScreen"/>
    <bean name="personnelManagementScreen" class="com.newdawn.gui.personnel.PersonnelManagementScreen"/>
    <bean name="personnelDetailsScreen" class="com.newdawn.gui.personnel.PersonnelDetailsPane"/>
    <bean name="teamManagementScreen" class="com.newdawn.gui.personnel.TeamManagementScreen"/>
    <bean name="teamAssignmentDialog" class="com.newdawn.gui.personnel.TeamAssignmentDialog"/>
</beans>
